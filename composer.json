{
    "name": "webfuelagency/socialite",
    "description": "This package aims to extend the functionalities of the Laravel Socialite package by adding new features or improving existing ones.",
    "type": "library",
    "homepage": "https://github.com/richard-galolo/socialite",
    "license": "MIT",
    "authors": [
        {
            "name": "Richard Galolo",
            "email": "richardgalolo@webfuelagency.com",
            "role": "developer"
        }
    ],
    "require": {
        "laravel/socialite": "v5.6.1",
        "doctrine/dbal": "3.6.1"
    },
    "require-dev": {
        "orchestra/testbench": "^8.2.0",
        "phpunit/phpunit": "^10.1@dev"
    },
    "autoload": {
        "psr-4": {
            "WebFuelAgency\\Socialite\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WebFuelAgency\\Socialite\\Tests\\": "tests",
            "WebFuelAgency\\Socialite\\Database\\Factories\\": "database/factories"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-f": "vendor/bin/phpunit --filter"
    },
    "extra": {
        "laravel": {
            "providers": [
                "WebFuelAgency\\Socialite\\SocialiteServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
